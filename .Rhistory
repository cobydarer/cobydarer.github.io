nhl_rink +
geom_point(data = pbp_24_hit_1, aes(x = x_fixed, y = y_fixed), alpha = .5) +
labs(caption = "1st Period Hits") +
theme(plot.caption = element_text(hjust = 0, size = 10))
nhl_rink +
geom_point(data = pbp_24_hit_2, aes(x = x_fixed, y = y_fixed), alpha = .5) +
